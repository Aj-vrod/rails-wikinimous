<div class="form">
  <%= form_for article do |f| %>
  <div class="field">
    <%= f.label :title %>
  <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :content %>
    <%= f.text_field :content %>
  </div>
  <div class="submit">
    <%= f.submit %>

  </div>
<% end %>
</div>
